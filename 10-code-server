clear
ip addr | grep eth0 | grep inet | awk -F ' ' '{print $2}'
if [ "`echo screen -ls | grep code-server`" == "" ];then
	screen -dmS code-server && screen -S code-server -X stuff 'code-server '`echo -ne '\015'`
	if [ "`echo screen -ls | grep code-server`" == "" ];then
		echo "[ERROR] : code-server not start. You can copy the command to try."
		echo "screen -dmS code-server && screen -S code-server -X stuff 'code-server '`echo -ne '\015'`"
	else
		echo "code-server start"
	fi
else	
	echo "exsit code-server"
fi